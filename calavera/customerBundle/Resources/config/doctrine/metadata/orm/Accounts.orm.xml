<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Accounts" table="accounts">
    <id name="id" type="string" column="id" length="36">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="150" nullable="true"/>
    <field name="dateEntered" type="datetime" column="date_entered" nullable="true"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    <field name="modifiedUserId" type="string" column="modified_user_id" length="36" nullable="true"/>
    <field name="createdBy" type="string" column="created_by" length="36" nullable="true"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
    <field name="assignedUserId" type="string" column="assigned_user_id" length="36" nullable="true"/>
    <field name="accountType" type="string" column="account_type" length="50" nullable="true"/>
    <field name="industry" type="string" column="industry" length="50" nullable="true"/>
    <field name="annualRevenue" type="string" column="annual_revenue" length="100" nullable="true"/>
    <field name="phoneFax" type="string" column="phone_fax" length="100" nullable="true"/>
    <field name="billingAddressStreet" type="string" column="billing_address_street" length="150" nullable="true"/>
    <field name="billingAddressCity" type="string" column="billing_address_city" length="100" nullable="true"/>
    <field name="billingAddressState" type="string" column="billing_address_state" length="100" nullable="true"/>
    <field name="billingAddressPostalcode" type="string" column="billing_address_postalcode" length="20" nullable="true"/>
    <field name="billingAddressCountry" type="string" column="billing_address_country" length="255" nullable="true"/>
    <field name="rating" type="string" column="rating" length="100" nullable="true"/>
    <field name="phoneOffice" type="string" column="phone_office" length="100" nullable="true"/>
    <field name="phoneAlternate" type="string" column="phone_alternate" length="100" nullable="true"/>
    <field name="website" type="string" column="website" length="255" nullable="true"/>
    <field name="ownership" type="string" column="ownership" length="100" nullable="true"/>
    <field name="employees" type="string" column="employees" length="10" nullable="true"/>
    <field name="tickerSymbol" type="string" column="ticker_symbol" length="10" nullable="true"/>
    <field name="shippingAddressStreet" type="string" column="shipping_address_street" length="150" nullable="true"/>
    <field name="shippingAddressCity" type="string" column="shipping_address_city" length="100" nullable="true"/>
    <field name="shippingAddressState" type="string" column="shipping_address_state" length="100" nullable="true"/>
    <field name="shippingAddressPostalcode" type="string" column="shipping_address_postalcode" length="20" nullable="true"/>
    <field name="shippingAddressCountry" type="string" column="shipping_address_country" length="255" nullable="true"/>
    <field name="parentId" type="string" column="parent_id" length="36" nullable="true"/>
    <field name="sicCode" type="string" column="sic_code" length="10" nullable="true"/>
    <field name="campaignId" type="string" column="campaign_id" length="36" nullable="true"/>
  </entity>
</doctrine-mapping>
