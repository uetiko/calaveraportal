<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Eapm" table="eapm">
    <id name="id" type="string" column="id" length="36">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true"/>
    <field name="dateEntered" type="datetime" column="date_entered" nullable="true"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    <field name="modifiedUserId" type="string" column="modified_user_id" length="36" nullable="true"/>
    <field name="createdBy" type="string" column="created_by" length="36" nullable="true"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
    <field name="assignedUserId" type="string" column="assigned_user_id" length="36" nullable="true"/>
    <field name="password" type="string" column="password" length="255" nullable="true"/>
    <field name="url" type="string" column="url" length="255" nullable="true"/>
    <field name="application" type="string" column="application" length="100" nullable="true"/>
    <field name="apiData" type="text" column="api_data" nullable="true"/>
    <field name="consumerKey" type="string" column="consumer_key" length="255" nullable="true"/>
    <field name="consumerSecret" type="string" column="consumer_secret" length="255" nullable="true"/>
    <field name="oauthToken" type="string" column="oauth_token" length="255" nullable="true"/>
    <field name="oauthSecret" type="string" column="oauth_secret" length="255" nullable="true"/>
    <field name="validated" type="boolean" column="validated" nullable="true"/>
  </entity>
</doctrine-mapping>
