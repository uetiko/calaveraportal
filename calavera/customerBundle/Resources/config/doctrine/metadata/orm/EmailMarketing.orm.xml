<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EmailMarketing" table="email_marketing">
    <id name="id" type="string" column="id" length="36">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
    <field name="dateEntered" type="datetime" column="date_entered" nullable="true"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    <field name="modifiedUserId" type="string" column="modified_user_id" length="36" nullable="true"/>
    <field name="createdBy" type="string" column="created_by" length="36" nullable="true"/>
    <field name="name" type="string" column="name" length="255" nullable="true"/>
    <field name="fromName" type="string" column="from_name" length="100" nullable="true"/>
    <field name="fromAddr" type="string" column="from_addr" length="100" nullable="true"/>
    <field name="replyToName" type="string" column="reply_to_name" length="100" nullable="true"/>
    <field name="replyToAddr" type="string" column="reply_to_addr" length="100" nullable="true"/>
    <field name="inboundEmailId" type="string" column="inbound_email_id" length="36" nullable="true"/>
    <field name="dateStart" type="datetime" column="date_start" nullable="true"/>
    <field name="templateId" type="string" column="template_id" length="36" nullable="false"/>
    <field name="status" type="string" column="status" length="100" nullable="true"/>
    <field name="campaignId" type="string" column="campaign_id" length="36" nullable="true"/>
    <field name="allProspectLists" type="boolean" column="all_prospect_lists" nullable="true"/>
  </entity>
</doctrine-mapping>
