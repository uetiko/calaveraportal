<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Emails" table="emails">
    <id name="id" type="string" column="id" length="36">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateEntered" type="datetime" column="date_entered" nullable="true"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    <field name="assignedUserId" type="string" column="assigned_user_id" length="36" nullable="true"/>
    <field name="modifiedUserId" type="string" column="modified_user_id" length="36" nullable="true"/>
    <field name="createdBy" type="string" column="created_by" length="36" nullable="true"/>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
    <field name="dateSent" type="datetime" column="date_sent" nullable="true"/>
    <field name="messageId" type="string" column="message_id" length="255" nullable="true"/>
    <field name="name" type="string" column="name" length="255" nullable="true"/>
    <field name="type" type="string" column="type" length="100" nullable="true"/>
    <field name="status" type="string" column="status" length="100" nullable="true"/>
    <field name="flagged" type="boolean" column="flagged" nullable="true"/>
    <field name="replyToStatus" type="boolean" column="reply_to_status" nullable="true"/>
    <field name="intent" type="string" column="intent" length="100" nullable="true"/>
    <field name="mailboxId" type="string" column="mailbox_id" length="36" nullable="true"/>
    <field name="parentType" type="string" column="parent_type" length="100" nullable="true"/>
    <field name="parentId" type="string" column="parent_id" length="36" nullable="true"/>
  </entity>
</doctrine-mapping>
