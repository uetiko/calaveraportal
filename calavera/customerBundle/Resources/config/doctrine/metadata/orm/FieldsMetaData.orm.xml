    <field name="audited" type="boolean" column="audited" nullable="true"/>
    <field name="massupdate" type="boolean" column="massupdate" nullable="true"/>
    <field name="duplicateMerge" type="smallint" column="duplicate_merge" nullable="true"/>
    <field name="reportable" type="boolean" column="reportable" nullable="true"/>
    <field name="importable" type="string" column="importable" length="255" nullable="true"/>
    <field name="ext1" type="string" column="ext1" length="255" nullable="true"/>
    <field name="ext2" type="string" column="ext2" length="255" nullable="true"/>
    <field name="ext3" type="string" column="ext3" length="255" nullable="true"/>
    <field name="ext4" type="text" column="ext4" nullable="true"/>
  </entity>
</doctrine-mapping>
