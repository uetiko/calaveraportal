<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FieldsMetaData" table="fields_meta_data">
    <id name="id" type="string" column="id" length="255">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true"/>
    <field name="vname" type="string" column="vname" length="255" nullable="true"/>
    <field name="comments" type="string" column="comments" length="255" nullable="true"/>
    <field name="help" type="string" column="help" length="255" nullable="true"/>
    <field name="customModule" type="string" column="custom_module" length="255" nullable="true"/>
    <field name="type" type="string" column="type" length="255" nullable="true"/>
    <field name="len" type="integer" column="len" nullable="true"/>
