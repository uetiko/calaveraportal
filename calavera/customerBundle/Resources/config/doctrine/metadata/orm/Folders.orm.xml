<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Folders" table="folders">
    <id name="id" type="string" column="id" length="36">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="25" nullable="true"/>
    <field name="folderType" type="string" column="folder_type" length="25" nullable="true"/>
    <field name="parentFolder" type="string" column="parent_folder" length="36" nullable="true"/>
    <field name="hasChild" type="boolean" column="has_child" nullable="true"/>
    <field name="isGroup" type="boolean" column="is_group" nullable="true"/>
    <field name="isDynamic" type="boolean" column="is_dynamic" nullable="true"/>
    <field name="dynamicQuery" type="text" column="dynamic_query" nullable="true"/>
    <field name="assignToId" type="string" column="assign_to_id" length="36" nullable="true"/>
    <field name="createdBy" type="string" column="created_by" length="36" nullable="false"/>
    <field name="modifiedBy" type="string" column="modified_by" length="36" nullable="false"/>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
  </entity>
</doctrine-mapping>
