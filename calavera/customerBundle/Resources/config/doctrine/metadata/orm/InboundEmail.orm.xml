<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="InboundEmail" table="inbound_email">
    <id name="id" type="string" column="id" length="36">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
    <field name="dateEntered" type="datetime" column="date_entered" nullable="true"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    <field name="modifiedUserId" type="string" column="modified_user_id" length="36" nullable="true"/>
    <field name="createdBy" type="string" column="created_by" length="36" nullable="true"/>
    <field name="name" type="string" column="name" length="255" nullable="true"/>
    <field name="status" type="string" column="status" length="100" nullable="true"/>
    <field name="serverUrl" type="string" column="server_url" length="100" nullable="true"/>
    <field name="emailUser" type="string" column="email_user" length="100" nullable="true"/>
    <field name="emailPassword" type="string" column="email_password" length="100" nullable="true"/>
    <field name="port" type="integer" column="port" nullable="true"/>
    <field name="service" type="string" column="service" length="50" nullable="true"/>
    <field name="mailbox" type="text" column="mailbox" nullable="true"/>
    <field name="deleteSeen" type="boolean" column="delete_seen" nullable="true"/>
    <field name="mailboxType" type="string" column="mailbox_type" length="10" nullable="true"/>
    <field name="templateId" type="string" column="template_id" length="36" nullable="true"/>
    <field name="storedOptions" type="text" column="stored_options" nullable="true"/>
    <field name="groupId" type="string" column="group_id" length="36" nullable="true"/>
    <field name="isPersonal" type="boolean" column="is_personal" nullable="true"/>
    <field name="groupfolderId" type="string" column="groupfolder_id" length="36" nullable="true"/>
  </entity>
</doctrine-mapping>
