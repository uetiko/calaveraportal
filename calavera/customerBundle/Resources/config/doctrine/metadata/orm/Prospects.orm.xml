<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Prospects" table="prospects">
    <id name="id" type="string" column="id" length="36">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateEntered" type="datetime" column="date_entered" nullable="true"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    <field name="modifiedUserId" type="string" column="modified_user_id" length="36" nullable="true"/>
    <field name="createdBy" type="string" column="created_by" length="36" nullable="true"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
    <field name="assignedUserId" type="string" column="assigned_user_id" length="36" nullable="true"/>
    <field name="salutation" type="string" column="salutation" length="255" nullable="true"/>
    <field name="firstName" type="string" column="first_name" length="100" nullable="true"/>
    <field name="lastName" type="string" column="last_name" length="100" nullable="true"/>
    <field name="title" type="string" column="title" length="100" nullable="true"/>
    <field name="department" type="string" column="department" length="255" nullable="true"/>
    <field name="doNotCall" type="boolean" column="do_not_call" nullable="true"/>
    <field name="phoneHome" type="string" column="phone_home" length="100" nullable="true"/>
    <field name="phoneMobile" type="string" column="phone_mobile" length="100" nullable="true"/>
    <field name="phoneWork" type="string" column="phone_work" length="100" nullable="true"/>
    <field name="phoneOther" type="string" column="phone_other" length="100" nullable="true"/>
    <field name="phoneFax" type="string" column="phone_fax" length="100" nullable="true"/>
    <field name="primaryAddressStreet" type="string" column="primary_address_street" length="150" nullable="true"/>
    <field name="primaryAddressCity" type="string" column="primary_address_city" length="100" nullable="true"/>
    <field name="primaryAddressState" type="string" column="primary_address_state" length="100" nullable="true"/>
    <field name="primaryAddressPostalcode" type="string" column="primary_address_postalcode" length="20" nullable="true"/>
    <field name="primaryAddressCountry" type="string" column="primary_address_country" length="255" nullable="true"/>
    <field name="altAddressStreet" type="string" column="alt_address_street" length="150" nullable="true"/>
    <field name="altAddressCity" type="string" column="alt_address_city" length="100" nullable="true"/>
    <field name="altAddressState" type="string" column="alt_address_state" length="100" nullable="true"/>
    <field name="altAddressPostalcode" type="string" column="alt_address_postalcode" length="20" nullable="true"/>
    <field name="altAddressCountry" type="string" column="alt_address_country" length="255" nullable="true"/>
    <field name="assistant" type="string" column="assistant" length="75" nullable="true"/>
    <field name="assistantPhone" type="string" column="assistant_phone" length="100" nullable="true"/>
    <field name="trackerKey" type="integer" column="tracker_key" nullable="false"/>
    <field name="birthdate" type="date" column="birthdate" nullable="true"/>
    <field name="leadId" type="string" column="lead_id" length="36" nullable="true"/>
    <field name="accountName" type="string" column="account_name" length="150" nullable="true"/>
    <field name="campaignId" type="string" column="campaign_id" length="36" nullable="true"/>
  </entity>
</doctrine-mapping>
