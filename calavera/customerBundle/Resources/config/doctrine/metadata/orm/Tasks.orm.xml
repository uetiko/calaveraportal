<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Tasks" table="tasks">
    <id name="id" type="string" column="id" length="36">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="true"/>
    <field name="dateEntered" type="datetime" column="date_entered" nullable="true"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    <field name="modifiedUserId" type="string" column="modified_user_id" length="36" nullable="true"/>
    <field name="createdBy" type="string" column="created_by" length="36" nullable="true"/>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
    <field name="assignedUserId" type="string" column="assigned_user_id" length="36" nullable="true"/>
    <field name="status" type="string" column="status" length="100" nullable="true"/>
    <field name="dateDueFlag" type="boolean" column="date_due_flag" nullable="true"/>
    <field name="dateDue" type="datetime" column="date_due" nullable="true"/>
    <field name="dateStartFlag" type="boolean" column="date_start_flag" nullable="true"/>
    <field name="dateStart" type="datetime" column="date_start" nullable="true"/>
    <field name="parentType" type="string" column="parent_type" length="255" nullable="true"/>
    <field name="parentId" type="string" column="parent_id" length="36" nullable="true"/>
    <field name="contactId" type="string" column="contact_id" length="36" nullable="true"/>
    <field name="priority" type="string" column="priority" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
