<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Tracker" table="tracker">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="monitorId" type="string" column="monitor_id" length="36" nullable="false"/>
    <field name="userId" type="string" column="user_id" length="36" nullable="true"/>
    <field name="moduleName" type="string" column="module_name" length="255" nullable="true"/>
    <field name="itemId" type="string" column="item_id" length="36" nullable="true"/>
    <field name="itemSummary" type="string" column="item_summary" length="255" nullable="true"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    <field name="action" type="string" column="action" length="255" nullable="true"/>
    <field name="sessionId" type="string" column="session_id" length="36" nullable="true"/>
    <field name="visible" type="boolean" column="visible" nullable="true"/>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
  </entity>
</doctrine-mapping>
