<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UsersFeeds" table="users_feeds">
    <id name="idUsersFeeds" type="integer" column="id_users_feeds">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="string" column="user_id" length="36" nullable="true"/>
    <field name="feedId" type="string" column="feed_id" length="36" nullable="true"/>
    <field name="rank" type="integer" column="rank" nullable="true"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="true"/>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
  </entity>
</doctrine-mapping>
