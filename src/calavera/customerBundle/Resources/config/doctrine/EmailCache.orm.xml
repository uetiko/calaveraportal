<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="calavera\customerBundle\Entity\EmailCache" table="email_cache">
    <id name="idEmailCache" type="integer" column="id_email_cache">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ieId" type="string" column="ie_id" length="36" nullable="true"/>
    <field name="mbox" type="string" column="mbox" length="60" nullable="true"/>
    <field name="subject" type="string" column="subject" length="255" nullable="true"/>
    <field name="fromaddr" type="string" column="fromaddr" length="100" nullable="true"/>
    <field name="toaddr" type="string" column="toaddr" length="255" nullable="true"/>
    <field name="senddate" type="datetime" column="senddate" nullable="true"/>
    <field name="messageId" type="string" column="message_id" length="255" nullable="true"/>
    <field name="mailsize" type="integer" column="mailsize" nullable="true"/>
    <field name="imapUid" type="integer" column="imap_uid" nullable="true"/>
    <field name="msgno" type="integer" column="msgno" nullable="true"/>
    <field name="recent" type="boolean" column="recent" nullable="true"/>
    <field name="flagged" type="boolean" column="flagged" nullable="true"/>
    <field name="answered" type="boolean" column="answered" nullable="true"/>
    <field name="deleted" type="boolean" column="deleted" nullable="true"/>
    <field name="seen" type="boolean" column="seen" nullable="true"/>
    <field name="draft" type="boolean" column="draft" nullable="true"/>
  </entity>
</doctrine-mapping>
